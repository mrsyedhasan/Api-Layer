{
  "name": "api-layer",
  "version": "1.0.0",
  "description": "API Layer Testing with Axios and Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:qa": "CONFIG_FILE=qa.config jest --verbose",
    "test:output": "jest --verbose --json --outputFile=test-results/test-output.json > test-results/test-output.txt 2>&1 || true && cat test-results/test-output.txt",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "api",
    "testing",
    "axios",
    "jest"
  ],
  "author": "Syed",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}

